    "proxy": "http://localhost:3007/",
    "dependencies": {
        "@microsoft/teams-js": "^1.5.2",
        "@stardust-ui/react": "^0.40.4",
        "axios": "^0.21.1",
        "i18next": "^19.0.2",
        "i18next-browser-languagedetector": "^4.0.1",
        "javascript-time-ago": "^2.0.4",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-i18next": "^11.2.6",
        "react-markdown": "^4.2.2",
        "react-redux": "^7.1.3",
        "react-router-dom": "^5.1.2",
        "redux": "^4.0.4"
    },
    "devDependencies": {
        "@types/jest": "24.0.20",
        "@types/node": "12.11.7",
        "@types/react": "16.9.11",
        "@types/react-dom": "16.9.3",
        "@types/react-redux": "^7.1.5",
        "@types/react-router-dom": "^5.1.3",
        "react-scripts": "^3.3.0",
        "react-snap": "^1.23.0",
        "redux-devtools": "^3.5.0",
        "sass": "^1.23.7",
        "typescript": "^3.7.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "postbuild": "react-snap",
        "test": "react-scripts test"
    },
    "repository": {
        "directory": "web",
        "type": "git",
        "url": "git+https://github.com/NewFuture/custom-stickers-teams-extension.git"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "reactSnap": {
        "skipThirdPartyRequests": true,
        "inlineCss": true,
        "cacheAjaxRequests": false,
        "include": [
            "/",
            "/config.html",
            "/privacy.html",
            "/terms.html"
        ],
        "exclude": [
            "/custom-stickers.zip"
        ],
        "minifyHtml": {
            "sortClassName": false
        },
        "viewport": {
            "width": 580,
            "height": 360
        },
        "crawl": false,
        "preloadImages": true
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
